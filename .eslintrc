{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "semi": [2, "never"],
    // "prettier/prettier": false
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": [
    "html"
  ],
  "settings": {
    "html/html-extensions": [".html"]
  }
}